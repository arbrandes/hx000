<%inherit file="lab.xml"/>\
<%def name="section_name()">Part 1 Lab</%def>\
<%block name="hastexo_tests"><%text>
    - |
      #!/bin/bash
      # Check for file
      file=foo
      if [ ! -e ${file} ]; then
        echo "File \"${file}\" doesn't exist." >&2
        exit 1
      fi
      exit 0
    - |
      #!/bin/bash
      # Check for file
      file=bar
      if [ ! -e ${file} ]; then
        echo "This is the first line." >&2
        echo "This is the second line, and it's a lot longer than the first one." >&2
        echo "File \"${file}\" doesn't exist." >&2
        exit 1
      fi
      exit 0
</%text></%block>
